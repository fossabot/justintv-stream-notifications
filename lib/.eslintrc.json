{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "babel"
    ],
    "rules": {
        "node/no-missing-require": 0,
        "node/no-unpublished-require": 0,
        "node/no-unsupported-features": 0,
        "no-console": 0,
        "generator-star-spacing": 0,
        "arrow-parens": 0,
        "babel/arrow-parens": 1,
        "babel/generator-star-spacing": 1
    },
    "extends": "plugin:freaktechnik/node"
}
